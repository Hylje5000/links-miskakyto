{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "NEXT_PUBLIC_AZURE_CLIENT_ID": "@azure_client_id",
    "NEXT_PUBLIC_AZURE_TENANT_ID": "@azure_tenant_id",
    "NEXT_PUBLIC_API_URL": "@api_url"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
